{
  "name": "cycling_repair_api",
  "version": "1.0.0",
  "description": "The symply Find_Bike_API",
  "main": "index.js",
  "scripts": {
    "start:dev": "node -r dotenv/config --inspect=0.0.0.0:9227 ./dist/",
    "start": "rimraf dist && npm run lint && copyfiles -e src/email-templates/*.ts -f src/email-templates/*.* dist/email-templates/ && tsc-watch --outDir ./dist --onSuccess \"npm run start:dev\" --onFailure \"'echo Compilation Failed'\"",
    "build": "rimraf dist && tsc",
    "tsc": "rimraf dist && npm run lint && tsc --watch",
    "lint": "eslint --fix --ext .ts,.js src"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.4",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.23.0",
    "inversify": "^5.0.1",
    "mongoose": "^5.9.19",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "^0.0.47",
    "@types/inversify": "^2.0.33",
    "@types/mongoose": "^5.7.24",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.14",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.22.0",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.8",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
